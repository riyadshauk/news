{
  "env": {
    "web": {
      "plugins": [
        "@babel/plugin-proposal-class-properties"
      ],
      "ignore": [
        "public/javascripts-node",
        "public/javascripts-web"
      ]
    },
    "node": {
      "plugins": [
        "@babel/plugin-transform-modules-commonjs",
        "@babel/plugin-proposal-class-properties"
      ],
      "ignore": [
        "public/javascripts-node",
        "public/javascripts-web"
      ]
    }
  }
}